{{> header}}

<main>
    <section class="info-area">
        <h1>To boldly go.</h1>
        <div class="info-body">
            <p>
			<table>
				<tr>
					<th> Episode </th>
					<th> Title </th>
					<th> Length </th>
				</tr>
				<tr>
				 {{#each episode}}
					  <td>{{episode}}</td>
					{{/each}}

				 {{#each ep_title}}
					  <td>{{ep_title}}</td>
					{{/each}}

				 {{#each len}}
					  <td>{{len}}</td>
					{{/each}}
					</tr>
			</table>
		</p>
        </div>
    </section>
</main>

{{> footer}}